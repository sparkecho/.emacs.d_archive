alias lps ps aux|grep -v grep|grep $* #look up process
alias javac javac -J-Dfile.encoding=utf8 $*
alias grep grep --color=auto $*
alias bz2 tar -xjvf $*          #decompress
alias tgz tar -xzvf $*          #decompress
alias gz tar -xzvf $*           #decompress
alias txz tar -xf $*            #decompress
alias ls /usr/bin/ls --color=auto $*
alias llh ls -lh $*
alias ll ls -ll $*
alias la ls -a $*
alias l ls $*
